{
  "name": "@jgarber/routerrouter",
  "version": "4.0.0",
  "description": "A very small JavaScript routing library extracted from Backboneâ€™s Router.",
  "keywords": [
    "backbone",
    "javascript",
    "router"
  ],
  "homepage": "https://github.com/jgarber623/RouterRouter",
  "bugs": "https://github.com/jgarber623/RouterRouter/issues",
  "license": "MIT",
  "author": {
    "name": "Jason Garber",
    "email": "jason@sixtwothree.org",
    "url": "https://sixtwothree.org"
  },
  "files": [
    "dist/*",
    "example/*",
    "src/*"
  ],
  "type": "module",
  "exports": {
    "import": "./dist/routerrouter.mjs",
    "require": "./dist/routerrouter.cjs"
  },
  "browser": "./dist/routerrouter.js",
  "directories": {
    "example": "example"
  },
  "repository": "github:jgarber623/RouterRouter",
  "scripts": {
    "build": "rollup --config",
    "clean": "rm -rf coverage dist",
    "lint": "eslint .",
    "prebuild": "npm run clean",
    "start": "concurrently --kill-others-on-fail --prefix-colors yellow,blue 'npm:start:*'",
    "start:build": "npm run build -- --watch",
    "start:serve": "http-server .",
    "test": "c8 ava"
  },
  "devDependencies": {
    "@jgarber/eslint-config": "^3.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "ava": "^5.3.1",
    "c8": "^8.0.1",
    "concurrently": "^8.2.1",
    "http-server": "^14.1.1",
    "jsdom": "^23.0.1",
    "rollup": "^4.0.2",
    "sinon": "^17.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
